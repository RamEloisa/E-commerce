<%= form_with(url: sessions_path) do %>
    <div>
        <%= label_tag "Username" %>
        <%= text_field_tag :user_name, nil, placeholder: "Username" %>
    </div>
    <div>
        <%= label_tag "Password" %>
        <%= password_field_tag :password, nil, placeholder: "Password" %>
    </div> 
    <div>
        <%= submit_tag "Log In"%>
    </div>
<% end %>